        "postcss-preset-env": "^9.1.4",
        "prettier": "^3.0.3",
        "rimraf": "^5.0.5",
        "style-loader": "^3.3.3",
        "typescript": "^5.2.2",
        "webpack": "^5.88.2",
        "webpack-cli": "^5.1.4"
    },
    "overrides-change-to-overrides-to-pin-child-dependencies-in-lock-file": {
