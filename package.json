	"type": "module",
	"main": "dist/ipywidgets.js",
	"files": [
		"dist/*.js",
		"ThirdPartyNotices-Repository.txt"
	],
	"scripts": {
		"Document": "Document 'Source/**/*.ts'",
		"analyze": "node scripts/postInstall.js &&  npm run clean && tsc && webpack --json > out/stats.json && npx webpack-bundle-analyzer out/stats.json -p=8981",
		"build": "node scripts/postInstall.js && npm run clean && tsc && webpack --mode=production",
		"build:dev": "node scripts/postInstall.js &&  npm run clean && tsc && webpack",
		"clean": "rimraf out && rimraf tsconfig.tsbuildinfo && rimraf dist",
		"prepack": "npm run clean && npm run build",
		"prepublishOnly": "Build 'Source/**/*.ts'"
	},
	"dependencies": {
		"@jupyter-widgets/base": "^6.0.2",
		"@jupyter-widgets/controls": "^5.0.3",
		"@jupyter-widgets/jupyterlab-manager": "^5.0.5",
		"@jupyter-widgets/output": "^6.0.2",
		"@jupyter-widgets/schema": "^0.5.1",
		"@jupyterlab/apputils": "^3.5.2",
		"@jupyterlab/docregistry": "^3.5.2",
		"@jupyterlab/notebook": "^3.5.2",
		"@jupyterlab/rendermime": "^3.5.2",
		"font-awesome": "^4.7.0"
	},
	"devDependencies": {
		"@playform/build": "0.0.7",
		"@playform/document": "0.0.6",
		"css-loader": "^6.7.1",
		"fs-extra": "^10.0.1",
		"isarray": "^2.0.5",
		"postcss": "^8.4.31",
		"postcss-import": "^14.0.2",
		"postcss-loader": "^6.2.1",
		"postcss-preset-env": "^7.4.2",
		"rimraf": "^3.0.2",
		"style-loader": "^3.3.1"
	},
	"publishConfig": {
		"access": "public"
	},
	"overrides-change-to-overrides-to-pin-child-dependencies-in-lock-file": {
		"json5": "2.2.3",
		"string_decoder": "1.3.0"
	}
}
